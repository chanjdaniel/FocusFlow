{
    "manifest_version": 3,
    "name": "FocusFlow",
    "description": "Improves readability of online text using bionic reading",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello_extensions.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["scripting", "activeTab"]
  }